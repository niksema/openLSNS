 <?xml version="1.0" encoding="UTF-8"?>
 <description>
	The library of ion channels
</description>
<!-- <include lib="libase.xml"/> import all modules from library "libase.xml" --> 
<include lib="libase.xml"/>
<template>
	<channels>
		<channel name="Na fast">
			<description>
				I = G*(V-Ena)
				G = Gmax*M^pm*H^ph
				where M - activation, pm - power of activation
					  H - inactivation, pm - power of inactivation
				Description of gate variables both activation and inactivation is taken from
				[#ref]
			</description>
			<parameters>
				<gmax name="Maximal conductance" import="/template/random/rnd" default="grnd">(0,0)</gmax>
				<eds ref="../ion[@name='Na ions']"/>
				<gate name="Activation" import="/template/gates/gate[@name='generic']">
					<parameters v12="43.8" slp="6." Tmax="0.252" slpT="10.8" pow="3"/>
					<parameters xpath="init">(0,0)</parameters>
				</gate>
				<gate name="Inactivation" import="/template/gates/gate[@name='generic']">
					<parameters v12="67.5" slp="10.8" Tmax="8.456" slpT="12.8" pow="1"/>
					<parameters xpath="init">(0,0)</parameters>
				</gate>
			</parameters>
		</channel>
		<channel name="Kdr">
			<description>
				I = G*(V-Ek)
				G = Gmax*M^pm
				where M - activation, pm - power of activation
				Description of activation is taken from
				[#ref]
			</description>
			<parameters>
				<gmax name="Maximal conductance" import="/template/random/rnd" default="grnd">(0,0)</gmax>
				<eds ref="../ion[@name='K ions']"/>
				<gate name="Activation" import="/template/gates/gate[@name='a-b']">
					<parameters "..."/>
					<parameters xpath="init">(0,0)</parameters>
				</gate>
			</parameters>
		</channel>
		<channel name="Leak current">
			<description>
				I = Gmax*(V-Eleak)
			</description>
			<parameters>
				<gmax name="Maximal conductance" import="/template/random/rnd" default="grnd">(0,0)</gmax>
				<eds ref="../ion[@name='Leak']"/>
			</parameters>
		</channel>
		<!-- etc --> 
	</channels>
</template>
